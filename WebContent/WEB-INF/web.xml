<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee www.oracle.com/webfolder/technetwork/jsc/xml/ns/javaee/web-app_3_1.xsd">

   <servlet>
        <servlet-name>DiscountDiscovery</servlet-name>
        <servlet-class>DiscountDiscovery</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DiscountDiscovery</servlet-name>
        <url-pattern>/DiscountDiscovery</url-pattern>
    </servlet-mapping>
  
  
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>

    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

</web-app>
